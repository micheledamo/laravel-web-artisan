{
    "name": "micheledamo/laravel-web-artisan",
    "type": "package",
    "description": "This package allows you to execute artisan commands in a simple way using the browser window. If you don't have SSH access to the hosting platform where your Laravel site is hosted, this solution allows you to run commands like \"php artisan migrate\".",
    "keywords": [
        "Michele Damo",
        "micheledamo",
        "laravel-web-artisan",
        "Laravel Web Atisan",
        "Artisan",
        "Artisan Commands",
        "Laravel"
    ],
    "homepage": "https://github.com/micheledamo/laravel-web-artisan",
    "license": "MIT",
    "authors": [
        {
            "name": "Michele Damo",
            "email": "hello@micheledamo.com",
            "homepage": "http://www.micheledamo.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "^5.0|^6.0|^7.0",
        "php" : ">=7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "laravel/framework": ">=5.8"
    },
    "autoload": {
        "psr-4": {
            "Micheledamo\\LaravelWebArtisan\\": "src"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Micheledamo\\LaravelWebArtisan\\LaravelWebArtisanServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
